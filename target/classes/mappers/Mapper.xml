<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.care.plusAir.repository.FlightDAO">
	<select id="selectCityAll" parameterType="com.care.plusAir.dto.SearchServiceDTO" resultType="com.care.plusAir.dto.FlightDTO">
		SELECT * FROM flight WHERE ${day}='Y'
		<if test="flightRouteNo != null">AND flightRouteNo like '%'||#{flightRouteNo}||'%'</if>
	</select>
</mapper>
